{
   "manifest_version": 2,
   "name": "Qiita stocker",
   "version": "0.2.0",
   "author": ["kawasima", "tksugimoto"],
   "description": "Enable to stock at Qiita",

   "content_scripts": [ {
      "js": [ "stock.js" ],
      "matches": [ "http://qiita.com/*" ]
   } ],
   "permissions": [ "https://qiita.com/" ],
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
  },
   "key": "",
   "update_url": "https://clients2.google.com/service/update2/crx"
}
